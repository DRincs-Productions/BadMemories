# game/events_m.rpy:67 #-#-# translate crowdin breakfastDay0_b1ac94f7:
msgid "[_mc_] Yeah, sure."
msgstr "[_mc_] Sì, certo."

# game/events_m.rpy:68 #-#-# translate crowdin breakfastDay0_6f4c46dc:
msgid "[_m_] Thanks, Hun."
msgstr "[_m_] Grazie, Hun."

# game/events_m.rpy:70 #-#-# translate crowdin breakfastDay0_bb2cf764:
