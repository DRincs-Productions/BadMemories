# game/events_m.rpy:107 #-#-# translate crowdin mTellNotToDisturb_2bc357cf:
msgid "[_mc_] Hey... Nothing special. I slept half the day."
msgstr "[_mc_] Ehi... Niente di speciale. Ho dormito per metà giornata."

# game/events_m.rpy:108 #-#-# translate crowdin mTellNotToDisturb_87b6d14e:
msgid "[_mcthink_] Does she ever wear a bra?"
