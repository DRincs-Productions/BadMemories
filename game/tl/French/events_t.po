# game/events_t.rpy:1373 #-#-# translate crowdin d10takekathome_e8b4d5fc:
msgid "[_mc_] So you got curious."
msgstr "[_mc_] Alors tu es curieuse."

# game/events_t.rpy:1375 #-#-# translate crowdin d10takekathome_9364ff8f:
