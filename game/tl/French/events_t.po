# game/events_t.rpy:1383 #-#-# translate crowdin d10takekathome_3c9d3ca8:
msgid "[_mc_] Depends on how you look at it."
msgstr "[_mc_] Ça dépend de la façon dont on voit les choses."

# game/events_t.rpy:1385 #-#-# translate crowdin d10takekathome_50f84d0d:
