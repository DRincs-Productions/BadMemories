# game/intro_day2.rpy:41 #-#-# translate crowdin intro_day2_1e2dd096:
msgid "Dad [special_delimiter] It's just a scratch, she's fine. Go to bed now."
msgstr "Papa [special_delimiter] C'est juste une égratigure, elle va bien. Va au lit maintenant."

# game/intro_day2.rpy:42 #-#-# translate crowdin intro_day2_d2cfcf43:
