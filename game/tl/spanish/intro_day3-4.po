# game/intro_day3-4.rpy:148 #-#-# translate crowdin intro_day3_6d197c55:
msgid "[_mc_] No, I'll just come over later."
msgstr "[_mc_] No, iré más tarde."

# game/intro_day3-4.rpy:149 #-#-# translate crowdin intro_day3_4e289dd8:
msgid "[_m_] Oh, okay, good."
msgstr "[_m_] Oh, vale, bien."

# game/intro_day3-4.rpy:150 #-#-# translate crowdin intro_day3_6fbbf689:
